<?xml version="1.0" encoding="UTF-8" ?><Shell    x:Class="MAUIProject.AppShell"    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:local="clr-namespace:MAUIProject"    xmlns:views="clr-namespace:MAUIProject.Views"    Title="MAUIProject">    <Shell.FlyoutHeader>        <Grid BackgroundColor="Black">
            <Image Aspect="AspectFill"
               Source="background.jpg"
               Opacity="0.7" />
            <Label Text="Company"
               TextColor="White"
               FontAttributes="Bold"
               HorizontalTextAlignment="Center"
               VerticalTextAlignment="Center" />
        </Grid>    </Shell.FlyoutHeader>    <Shell.FlyoutFooter>        <StackLayout             BackgroundColor="AliceBlue" >            <Label                 Text="Copyright"                 FontAttributes="Bold"                 Padding="10"                 FontSize="14"                 HorizontalOptions="Center"                 VerticalOptions="Center"/>        </StackLayout>    </Shell.FlyoutFooter>    <FlyoutItem         Title="Software Engineers"         Icon="user.png">        <Tab             Title="Software Engineers"             Icon="user.png">            <ShellContent                 Title="Trainee"                 ContentTemplate="{DataTemplate views:Trainee}"/>            <ShellContent                 Title="TSE"                 ContentTemplate="{DataTemplate views:TSE}"/>            <ShellContent                 Title="SE"                 ContentTemplate="{DataTemplate views:SE}"/>        </Tab>        <Tab             Title="Account"             Icon="user.png">            <ShellContent                 Title="Account"                 ContentTemplate="{DataTemplate views:Account}"/>        </Tab>    </FlyoutItem>    <FlyoutItem         Title="Leads"         Icon="user.png">        <Tab             Title="Lead">            <ShellContent                 Title="Senior Team Lead"                 ContentTemplate="{DataTemplate views:SeniorTeamLead}"/>            <ShellContent                 Title="Team Lead"                 ContentTemplate="{DataTemplate views:TeamLead}"/>        </Tab>    </FlyoutItem>    <FlyoutItem         Title="Project Managers"         Icon="user.png">        <ShellContent             Title="PM"             ContentTemplate="{DataTemplate views:ProjectManager}"/>    </FlyoutItem>    <MenuItem         Text="Help"         IconImageSource="help.png" /></Shell>