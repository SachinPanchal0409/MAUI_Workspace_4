<?xml version="1.0" encoding="UTF-8" ?><Shell    x:Class="MAUIProject.AppShell"    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:local="clr-namespace:MAUIProject"    xmlns:views="clr-namespace:MAUIProject.Views"    Title="MAUIProject">    <Shell.FlyoutHeader>        <Grid BackgroundColor="Black">
            <Image Aspect="AspectFill"
               Source="background.jpg"
               Opacity="0.7" />
            <Label Text="Company"
               TextColor="White"
               FontAttributes="Bold"
               HorizontalTextAlignment="Center"
               VerticalTextAlignment="Center" />
        </Grid>    </Shell.FlyoutHeader>    <Shell.FlyoutFooter>        <StackLayout             BackgroundColor="AliceBlue" >            <Label                 Text="Copyright"                 FontAttributes="Bold"                 Padding="10"                 FontSize="14"                 HorizontalOptions="Center"                 VerticalOptions="Center"/>        </StackLayout>    </Shell.FlyoutFooter>    <FlyoutItem         Title="Software Engineers"         Icon="user.png">        <Tab             Title="Software Engineers"             Icon="user.png">            <ShellContent                 Title="Trainee"                 ContentTemplate="{DataTemplate views:Trainee}"/>            <ShellContent                 Title="TSE"                 ContentTemplate="{DataTemplate views:TSE}"/>            <ShellContent                 Title="SE"                 ContentTemplate="{DataTemplate views:SE}"/>        </Tab>        <Tab             Title="Account"             Icon="user.png">            <ShellContent                 Title="Account"                 ContentTemplate="{DataTemplate views:Account}"/>        </Tab>    </FlyoutItem>    <FlyoutItem         Title="Leads"         Icon="user.png">        <Tab             Title="Lead">            <ShellContent                 Title="Senior Team Lead"                 ContentTemplate="{DataTemplate views:SeniorTeamLead}"/>            <ShellContent                 Title="Team Lead"                 ContentTemplate="{DataTemplate views:TeamLead}"/>        </Tab>    </FlyoutItem>    <FlyoutItem         Title="Project Managers"         Icon="user.png">        <ShellContent             Title="PM"             ContentTemplate="{DataTemplate views:ProjectManager}"/>    </FlyoutItem>    <FlyoutItem          Title="Layouts"          Icon="user.png">        <Tab            Title="Grid Layout">            <ShellContent                 Title="Example 1"                 ContentTemplate="{DataTemplate views:GridLayout}"/>            <ShellContent                 Title="Example 2"                 ContentTemplate="{DataTemplate views:GridLayoutExample}"/>        </Tab>        <ShellContent            Title="Flex Layout"            ContentTemplate="{DataTemplate views:FlexLayout}"/>        <ShellContent            Title="Stack Layout"            ContentTemplate="{DataTemplate views:StackLayout}"/>        <ShellContent            Title="Absolute Layout"            ContentTemplate="{DataTemplate views:AbsoluteLayout}"/>    </FlyoutItem>    <FlyoutItem          Title="Refresh View"          Icon="user.png">        <ShellContent          Title="Refresh View"          ContentTemplate="{DataTemplate views:RefreshViewExample}"/>    </FlyoutItem>    <FlyoutItem           Title="Progress Bar"           Icon="user.png">        <ShellContent       Title="Progress Bar"       ContentTemplate="{DataTemplate views:ProgressBarExample}"/>    </FlyoutItem>    <FlyoutItem           Title="Buttons"           Icon="user.png">        <ShellContent           Title="Buttons"           ContentTemplate="{DataTemplate views:ButtonsExample}"/>    </FlyoutItem>    <FlyoutItem       Title="Collection View"       Icon="user.png">        <ShellContent           Title="Collection View"           ContentTemplate="{DataTemplate views:CollectionViewExample}"/>    </FlyoutItem>    <FlyoutItem       Title="List View"       Icon="user.png">        <ShellContent       Title="List View"       ContentTemplate="{DataTemplate views:ListViewExample}"/>    </FlyoutItem>    <FlyoutItem       Title="Carousel View"       Icon="user.png">        <ShellContent       Title="Carousel View"       ContentTemplate="{DataTemplate views:CarouselViewExample}"/>    </FlyoutItem>    <FlyoutItem       Title="Rating Bar"       Icon="user.png">        <ShellContent       Title="Rating Bar"       ContentTemplate="{DataTemplate views:RatingBarExample}"/>    </FlyoutItem>    <FlyoutItem    Title="Notification"    Icon="user.png">        <ShellContent    Title="Notification"    ContentTemplate="{DataTemplate views:NotificationExample}"/>    </FlyoutItem>    <FlyoutItem         Title="Maps"         Icon="user.png">        <ShellContent             Title="Maps"             ContentTemplate="{DataTemplate views:MapExample}"/>    </FlyoutItem>    <FlyoutItem      Title="Biometric"      Icon="user.png">        <ShellContent           Title="Biometric"           ContentTemplate="{DataTemplate views:BiometricExample}"/>        <ShellContent          Title="Biometric 2"          ContentTemplate="{DataTemplate views:BiometricAlternativeExample}"/>    </FlyoutItem>    <MenuItem         Text="Help"         IconImageSource="help.png" /></Shell>